{
  "name": "bcp-interactive-wizard",
  "version": "1.0.0",
  "description": "Business Continuity Planning Interactive Wizard",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "seed": "cd server && node seed.js",
    "test-api": "node test-api.js",
    "test-flow": "node test-full-flow.js",
    "start": "cd server && npm start",
    "start-dev": "./start-dev.sh"
  },
  "keywords": ["bcp", "business-continuity", "wizard", "react", "nodejs"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "axios": "^1.5.0"
  }
}